{
  "build": {
    "beforeBuildCommand": "node esbuild.build.mjs",
    "beforeDevCommand": "node esbuild.dev.mjs",
    "devPath": "http://127.0.0.1:5500/public/",
    "distDir": "./public"
  },
  "package": {
    "productName": "cyblog",
    "version": "2.0.0"
  },
  "tauri": {
    "allowlist": {
      "dialog": {
        "all": true
      },
      "path": {
        "all": true
      },
      "all": true,
      "fs": {
        "scope": ["**"]
      }
    },
    "bundle": {
      "active": true,
      "category": "DeveloperTool",
      "copyright": "Copyright ©️ 2023- Siddharth Singh",
      "deb": {
        "depends": []
      },
      "externalBin": [],
      "icon": [
        "icons/32x32.png",
        "icons/128x128.png",
        "icons/128x128@2x.png",
        "icons/icon.icns",
        "icons/icon.ico"
      ],
      "identifier": "xyz.shantaram.cyblog",
      "longDescription": "Cybernetically enhanced static site generation.",
      "macOS": {
        "entitlements": null,
        "exceptionDomain": "",
        "frameworks": [],
        "providerShortName": null,
        "signingIdentity": null
      },
      "resources": [],
      "shortDescription": "Cybernetically enhanced static site generation.",
      "targets": ["appimage", "deb", "msi"],
      "windows": {
        "certificateThumbprint": null,
        "digestAlgorithm": "sha256",
        "timestampUrl": ""
      }
    },
    "security": {
      "csp": null
    },
    "updater": {
      "active": false
    },
    "windows": [
      {
        "fullscreen": false,
        "height": 1024,
        "resizable": true,
        "title": "Cyblog",
        "width": 768
      }
    ]
  }
}
